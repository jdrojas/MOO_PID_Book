file = {:home/david/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ortiz et al. - 2011 - Improving the visualization of the Pareto-optimal front for the multi-response optimization of chromatographic det.pdf:pdf},
issn = {0003-2670},
journal = {Analytica Chimica Acta},
month = {feb},
number = {2},
pages = {129--136},
publisher = {Elsevier},
title = {{Improving the visualization of the Pareto-optimal front for the multi-response optimization of chromatographic determinations}},
