pages = {505--522},
publisher = {Elsevier B.V.},
title = {{An a posteriori decision support methodology for solving the multi-criteria supplier selection problem}},
url = {https://doi.org/10.1016/j.ejor.2018.06.044 https://linkinghub.elsevier.com/retrieve/pii/S0377221718305964},
volume = {272},
