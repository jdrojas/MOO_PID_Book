@article{Arrieta2010,
abstract = {This paper analyzes optimal controller settings for controllers with
One-Degree-of-Freedom (1-DoF) Proportional-Integral-Derivative (PID)
structure. A new analysis is conducted from the point of view of
the operating mode (either servo or regulation mode) of the control-loop
