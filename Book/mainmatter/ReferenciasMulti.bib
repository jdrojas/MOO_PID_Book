@article{Gyoengy2006,
abstract = {A simple approach to the automatic tuning of PID process controllers
is proposed. Like the relay-based autotuner, its objective is to
attain a design-point on the Nyquist diagram. By injecting sinewaves
and employing a phase/frequency estimator, closed-loop adaptive tuning
