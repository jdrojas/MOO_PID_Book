file = {::},
issn = {2352152X},
journal = {Journal of Energy Storage},
keywords = {Energy losses,Energy storage systems,Multi-objective,Optimal storage locations,Optimization,Post-Pareto analysis,Radial distribution systems,Reactive-power compensation,Sum of ranking differences},
month = {aug},
number = {January},
pages = {100769},
publisher = {Elsevier},
title = {{Multi-objective optimization with post-pareto optimality analysis for the integration of storage systems with reactive-power compensation in distribution networks}},
url = {https://doi.org/10.1016/j.est.2019.100769 https://linkinghub.elsevier.com/retrieve/pii/S2352152X1930074X},
volume = {24},
